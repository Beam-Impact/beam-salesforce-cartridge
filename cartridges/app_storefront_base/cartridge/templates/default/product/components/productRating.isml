<isloop items="${[1, 2, 3, 4, 5]}" var="star">
    <i class="fa
        <isif condition="${product.rating >= star}">
            fa-star
        <iselse/>
            <isif condition="${parseInt(product.rating) < star}">
                fa-star-o
            <iselse/>
                fa-star-half-o
            </isif>
        </isif>
        " aria-hidden="true">
    </i>
</isloop>
