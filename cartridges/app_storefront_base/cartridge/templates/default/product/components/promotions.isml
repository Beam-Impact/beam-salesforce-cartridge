<div class="row promotions">
    <div class="banner col-xs-12">
        <div class="promotion">
            <isloop items="${pdict.product.promotions}" var="promotion">
                <div class="callout">
                    ${promotion.calloutMsg}
                </div>
                <div class="details">
                    ${promotion.details}
                </div>
            </isloop>
        </div>
    </div>
</div>
