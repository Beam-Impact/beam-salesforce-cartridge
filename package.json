{
  "name": "mfsg",
  "version": "3.0.0",
  "description": "Mobile First SiteGenesis",
  "main": "index.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "test:appium": "node bin/Makefile functional -- appium -- ",
    "cover": "sgmf-scripts --cover",
    "test:integration": "node bin/Makefile integration -- ",
    "test:functional": "node bin/Makefile functional -- ",
    "test:functional:docker": "bin/test-functional-docker.sh",
    "test:functional:sauce": "./node_modules/.bin/wdio test/functional/webdriver/wdio.conf.js ",
    "compile:scss": "sgmf-scripts --compile css",
    "compile:js": "sgmf-scripts --compile js",
    "compile:fonts": "node bin/Makefile compileFonts",
    "auth": "./node_modules/.bin/sfcc-ci client:auth",
    "data:zip": "cd node_modules/refarchdata/; zip -r -q demo_data_sfra.zip demo_data_sfra; mv demo_data_sfra.zip ../../demo_data_sfra.zip",
    "data:upload": "./node_modules/.bin/sfcc-ci instance:upload demo_data_sfra.zip",
    "data:import": "./node_modules/.bin/sfcc-ci instance:import demo_data_sfra.zip -s",
    "data:rebuildurls": "./node_modules/.bin/sfcc-ci job:run RebuildURLs -s",
    "data:reindex": "./node_modules/.bin/sfcc-ci job:run Reindex -s",
    "data:update": "npm run data:zip; npm run auth; npm run data:upload; npm run data:import; rm demo_data_sfra.zip; npm run data:reindex; npm run data:rebuildurls; ",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "sgmf-scripts --lint css",
    "lint:js": "sgmf-scripts --lint js",
    "code:zip": "mkdir -p tmp/code; cp -R cartridges/* tmp/code; cd tmp; zip -r -q code.zip code; rm -r code",
    "code:upload": "./node_modules/.bin/sfcc-ci code:deploy tmp/code.zip ",
    "code:activate": "./node_modules/.bin/sfcc-ci code:activate code ",
    "code:update": "npm run compile:scss; npm run compile:js; npm run compile:fonts; npm run code:zip; npm run auth; npm run code:upload; npm run code:activate; rm tmp/code.zip",
    "watch": "sgmf-scripts --watch",
    "release": "node bin/Makefile release --"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/demandware/storefront-base.git"
  },
  "author": "Ilya Volodin <ivolodin@demandware.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/demandware/storefront-base#readme",
  "devDependencies": {
    "@tridnguyen/config": "^2.3.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.5.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "cldr-data": "^31.0.3",
    "dwupload": "^3.4.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "globalize": "^1.1.1",
    "istanbul": "^0.4.4",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.17.0",
    "moment-timezone": "^0.5.21",
    "properties-parser": "^0.3.0",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.2",
    "selenium-standalone": "^5.9.0",
    "sfcc-ci": "git+http://sfccbuild:Z54TX2qk8JzA4kAmU8pD@bitbucket.org/demandware/sfcc-ci.git#ccdx-integration",
    "sgmf-scripts": "^1.1.3",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.4",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.5.0",
    "url": "^0.11.0",
    "wdio-sauce-service": "~0.1",
    "webdriverio": "^3.3.0",
    "xml2js": "^0.4.17"
  },
  "dependencies": {
    "bootstrap": "4.0.0",
    "cleave.js": "^1.3.7",
    "flag-icon-css": "^2.3.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "npm": "^6.1.0",
    "refarchdata": "git+http://sfccbuild:Z54TX2qk8JzA4kAmU8pD@bitbucket.org/demandware/storefrontdata.git#ci-base-jobs"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "packageName": "app_storefront_base"
}
