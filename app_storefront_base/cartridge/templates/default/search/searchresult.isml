<isdecorate template="common/layout/page">
    <div class="container">
    	<!--- to display Product IDs --->
    	<div>
	        <isloop items="${pdict.productIds}" var="productId" status="loopStatus">
	        	${productId}
	        </isloop>
		</div>

        <!--- to display relevant Refinement Definition Names --->
        <div>
        	<isloop items="${pdict.refinementNames}" var="refinementName">
        		${refinementName}
        	</isloop>
        </div>
    </div>
</isdecorate>
